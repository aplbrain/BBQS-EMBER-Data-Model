{
    "$defs": {
        "Contributor": {
            "additionalProperties": false,
            "description": "Person or organization contributing to the project",
            "properties": {
                "contributor_id": {
                    "description": "Internal identifier for the contributor within EMBER metadata",
                    "type": "string"
                },
                "email": {
                    "description": "Contact email address",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "external_identifiers": {
                    "description": "External identifiers such as ORCID",
                    "items": {
                        "type": "string"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "institution": {
                    "description": "Institutional affiliation",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "name": {
                    "description": "Full name of the contributor",
                    "type": "string"
                }
            },
            "required": [
                "contributor_id",
                "name"
            ],
            "title": "Contributor",
            "type": "object"
        },
        "EmberProject": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "access_level_access_vault_ids": {
                    "description": "embervault ids",
                    "items": {
                        "type": "string"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "access_level_emberdandisets": {
                    "description": "emberdandiset ids",
                    "items": {
                        "pattern": "^EMBER-DANDI:\\d{6}$",
                        "type": "string"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "access_level_restricted_datasets": {
                    "description": "restricted dataset ids",
                    "items": {
                        "type": "string"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "access_tier_summary": {
                    "description": "text description of different data tiers",
                    "type": "string"
                },
                "data_administrator": {
                    "$ref": "#/$defs/Contributor",
                    "description": "Primary point of contact for data management"
                },
                "data_availability_emberdandi": {
                    "description": "yes/no indicating if data are available in emberdandi",
                    "type": "boolean"
                },
                "data_availability_emberrestricted": {
                    "description": "yes/no indicating if data are available in emberrestricted",
                    "type": "boolean"
                },
                "data_availability_embervault": {
                    "description": "yes/no indicating if PHI/PII data in embervault",
                    "type": "boolean"
                },
                "data_use_agreement": {
                    "description": "Description or reference to the data use agreement governing access to restricted or controlled data",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "data_use_agreement_required": {
                    "description": "Indicates whether a data use agreement is required to access any project data",
                    "type": [
                        "boolean",
                        "null"
                    ]
                },
                "ember_doi": {
                    "anyOf": [
                        {
                            "pattern": "^10\\.\\d{4,9}/[-._;()/:A-Z0-9]+$",
                            "type": "string"
                        },
                        {
                            "const": "",
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "DOI in EMBER system (empty string allowed until assigned)",
                    "type": "string"
                },
                "funding": {
                    "description": "Funding sources supporting this project",
                    "items": {
                        "$ref": "#/$defs/Funding"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "last_metadata_update": {
                    "description": "date and time of last metadata update",
                    "type": "string"
                },
                "metadata_version": {
                    "description": "version of the metadata",
                    "type": "string"
                },
                "project_description": {
                    "description": "text description of the project",
                    "type": "string"
                },
                "project_id": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "project_title": {
                    "description": "title of the project",
                    "type": "string"
                },
                "related_dandisets": {
                    "description": "DOIs for related dandisets",
                    "items": {
                        "type": "string"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "related_data": {
                    "description": "Identifiers for other related datasets",
                    "items": {
                        "type": "string"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "related_publications": {
                    "description": "Publications associated with this project",
                    "items": {
                        "$ref": "#/$defs/Publication"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "related_repositories": {
                    "description": "Identifiers for related repositories",
                    "items": {
                        "type": "string"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                }
            },
            "required": [
                "project_id",
                "project_title",
                "project_description",
                "data_availability_emberdandi",
                "data_availability_emberrestricted",
                "data_availability_embervault",
                "access_tier_summary",
                "data_administrator",
                "last_metadata_update",
                "metadata_version"
            ],
            "title": "EmberProject",
            "type": "object"
        },
        "Funding": {
            "additionalProperties": false,
            "description": "Funding source information for the project",
            "properties": {
                "award_number": {
                    "description": "Funding award or grant number",
                    "type": "string"
                },
                "award_title": {
                    "description": "Title of the funded award",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "funding_institute": {
                    "description": "Name of the funding institute (e.g., NIH, NSF, DARPA)",
                    "type": "string"
                },
                "funding_url": {
                    "description": "URL to the funding announcement or award page",
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "required": [
                "funding_institute",
                "award_number"
            ],
            "title": "Funding",
            "type": "object"
        },
        "Publication": {
            "additionalProperties": false,
            "description": "Scholarly publication associated with the project",
            "properties": {
                "authors": {
                    "description": "Authors of the publication (references to Contributor objects)",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "journal": {
                    "description": "Journal or venue where the work was published",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "publication_doi": {
                    "anyOf": [
                        {
                            "pattern": "^10\\.\\d{4,9}/[-._;()/:A-Z0-9]+$",
                            "type": "string"
                        },
                        {
                            "const": "",
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "DOI for the publication (empty string allowed if pending)",
                    "type": "string"
                },
                "publication_url": {
                    "description": "URL to the publication landing page or preprint",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "title": {
                    "description": "Title of the publication",
                    "type": "string"
                },
                "year": {
                    "description": "Year of publication",
                    "type": [
                        "integer",
                        "null"
                    ]
                }
            },
            "required": [
                "title",
                "authors"
            ],
            "title": "Publication",
            "type": "object"
        }
    },
